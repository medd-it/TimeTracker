<table>
  <tr>
    <th>User</th>
    <td><%= f.select(:user_id, User.all.map {|u| [u.e_mail, u.id] }) %></td>
  </tr>
  <tr>
    <th>Project</th>
    <td><%= f.select(:project_id, Project.all.map {|p| [p.code, p.id] }) %></td>
  </tr>
  <tr>
    <th>Start</th>
    <td><%= f.datetime_field(:start) %></td>
  </tr>
  <tr>
    <th>End</th>
    <td><%= f.datetime_field(:end) %></td>
  </tr>
  <tr>
    <th>Notes</th>
    <td><%= f.text_field(:notes) %></td>
  </tr>
</table>