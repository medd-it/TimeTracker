<div>
  <h2>Show Timesheet</h2>

  <div>
    <%= link_to('Edit', edit_timesheet_path(@timesheet.id)) %>
    <%= link_to('Delete', delete_timesheet_path(@timesheet.id)) %>
  </div>

  <table>
    <tr>
      <th>User</th>
      <td><%= @timesheet.user.e_mail %></td>
    </tr>
    <tr>
      <th>Project</th>
      <td><%= @timesheet.project.code %></td>
    </tr>
    <tr>
      <th>Start</th>
      <td><%= @timesheet.start %></td>
    </tr>
    <tr>
      <th>End</th>
      <td><%= @timesheet.end %></td>
    </tr>
    <tr>
      <th>Notes</th>
      <td><%= @timesheet.notes %></td>
    </tr>
  </table>

</div>